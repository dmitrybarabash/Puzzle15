<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <LangVersion>latest</LangVersion>
    <UseWindowsForms>true</UseWindowsForms>

    <NoWarn>
      WFO1000 <!-- Property does not configure the code serialization for its property content -->
    </NoWarn>

    <Authors>Dmitry Barabash</Authors>
    <Company>Dmitry Barabash's Software Company</Company>
    <Description>Puzzle 15 Game</Description>
    <Copyright>Copyright © 2015-2025 Dmitry Barabash</Copyright>
    <Version>1.4.7.2</Version>
    <AssemblyVersion>1.4.7.2</AssemblyVersion>
    <FileVersion>1.4.7.2</FileVersion>
    <ApplicationIcon>AppIcon.ico</ApplicationIcon>
    <PackageIcon>AppIcon.ico</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Puzzle15.Common\Puzzle15.Common.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Resources\AppIcon.ico">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="Resources\LogoImage.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>